{
  "name": "@gbaud/kibana-client",
  "version": "1.0.3",
  "description": "Kibana client for Kibana API usage",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronx12/kibana-client.git"
  },
  "keywords": [
    "kibana",
    "client",
    "api",
    "elastic",
    "security"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kronx12/kibana-client/issues"
  },
  "homepage": "https://github.com/Kronx12/kibana-client#readme",
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^1.2.1"
  }
}
